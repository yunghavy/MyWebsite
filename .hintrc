{
  "extends": [
    "development"
  ],
  "hints": {
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.4",
    "not safari <= 16.4"
  ]
}